  "manifest_version": 3,
  "action": {
    "default_popup": "layout/popup.html",
    "default_icon": {
      "16": "images/icons/stopped-16.png",
      "32": "images/icons/stopped-32.png",
      "64": "images/icons/stopped-64.png",
      "128": "images/icons/stopped-128.png"
